pipeline {
    agent any
    stages {
        stage('Récupérer le code source') {
            steps {
                git branch: 'main', url: 'https://github.com/ABDELILAH098/987.git'
            }
        }
        
        stage('Installer les dépendances') {
            steps {
                sh 'npm install'
            }
        }
        
        stage('Build de l\'application') {
            steps {
                sh 'npm run build'
            }
        }
        
        // Ajoutez les autres étapes de votre pipeline ici
    }
}
